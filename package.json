{
	"name": "header-source-side-by-side",
	"displayName": "Header/Source Side-By-Side",
	"description": "Opens header & source in a split next to each other",
	"publisher": "JosephLyncheski",
	"repository": {
		"type": "git",
		"url": "https://github.com/directmusic/header-source-side-by-side"
	},
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:header-source-side-by-side.do"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "header-source-side-by-side.do",
				"title": "Split Header/Source horizontally."
			}
		],
		"keybindings": [
			{
				"command": "header-source-side-by-side.do",
				"key": "ctrl+alt+shift+up",
				"mac": "cmd+alt+ctrl+up"
			},
			{
				"command": "header-source-side-by-side.do",
				"key": "ctrl+alt+shift+down",
				"mac": "cmd+alt+ctrl+down"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.3"
	}
}